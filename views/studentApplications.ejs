<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('./partials/head') %>
</head>

<body>
  <header>
    <%- include('./partials/header') %>
  </header>
  <div style="padding-left:20px">

    <div class="job-posting">
      <% applications.forEach(function(application, index) { %>
        <div class="job-card">
          <div class="card5">
            <h2>
              <%= application.title %>
            </h2>
            <p>
              <b> Email: </b>
              <br>
              <%= application.emailaddress %> <br>
            </p>
            <p>
              <b> Type: </b>
              <br>
              <%= application.dateapplied %> <br>
            </p>
    
          </div>
        </div>
    </div>
    <% }); %>
  </div>
  <footer>
    <%- include('./partials/footer') %>
  </footer>
</body>